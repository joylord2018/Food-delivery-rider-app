{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/services/apiService.ts",
    "src/services/storageService.ts",
    "src/mock/index.ts"
  ],
  "out": "docs",
  "name": "Trae Solo API Documentation",
  "theme": "default",
  "excludeExternals": true,
  "includeVersion": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "readme": "README.md",
  "hideGenerator": false,
  "navigationLinks": {
    "GitHub": "https://github.com/yourusername/trae-solo"
  },
  "plugin": ["typedoc-plugin-markdown"],
  "tsconfig": "tsconfig.typedoc.json"
}